{
    "name": "laravel-workos/laravel-workos",
    "description": "Comprehensive Laravel WorkOS authentication package with session-based, JWT API, and server-to-server authentication support",
    "keywords": [
        "laravel",
        "workos",
        "authentication",
        "sso",
        "jwt",
        "oauth",
        "authkit",
        "rbac"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Laravel WorkOS Team",
            "email": "support@laravel-workos.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "workos/workos-php": "^4.0",
        "firebase/php-jwt": "^6.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/auth": "^9.0|^10.0|^11.0",
        "illuminate/http": "^9.0|^10.0|^11.0",
        "illuminate/cache": "^9.0|^10.0|^11.0",
        "illuminate/session": "^9.0|^10.0|^11.0",
        "illuminate/cookie": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.0",
        "mockery/mockery": "^1.5",
        "laravel/pint": "^1.0",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelWorkOS\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelWorkOS\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/pint",
        "analyse": "vendor/bin/phpstan analyse"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelWorkOS\\WorkOSServiceProvider"
            ],
            "aliases": {
                "WorkOS": "LaravelWorkOS\\Facades\\WorkOS",
                "WorkOSAuth": "LaravelWorkOS\\Facades\\WorkOSAuth",
                "WorkOSUser": "LaravelWorkOS\\Facades\\WorkOSUser",
                "WorkOSPermissions": "LaravelWorkOS\\Facades\\WorkOSPermissions"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}